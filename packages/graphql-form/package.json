{
    "name": "graphql-form",
    "version": "0.0.24",
    "description": "Easy form creation with GraphQL Editor and React",
    "main": "lib/index.js",
    "scripts": {
        "test": "jest --maxWorkers=1",
        "test:watch": "jest --watch --maxWorkes=1",
        "build": "tsc -p tsconfig.build.json",
        "start": "tsc -p tsconfig.build.json --watch",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "a": "node ./lib/GqlToForm"
    },
    "types": "lib/index.d.ts",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/graphql-editor/graphql-form.git"
    },
    "author": "Aexol <aexol@aexol.com> (http://aexol.com)",
    "license": "MIT",
    "homepage": "https://form.graphqleditor.com",
    "devDependencies": {
        "graphql": "^16.5.0"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "dependencies": {
        "graphql-js-tree": "^0.1.1"
    }
}
